{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "img/icons/omega-32.png",
      "default_popup": "popup.html",
      "default_title": "__MSG_manifest_icon_default_title__"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Alt+Shift+O"
         }
      }
   },
   "default_locale": "zh_CN",
   "description": "__MSG_manifest_app_description__",
   "icons": {
      "128": "img/icons/omega-128.png",
      "16": "img/icons/omega-16.png",
      "32": "img/icons/omega-32.png",
      "48": "img/icons/omega-48.png",
      "64": "img/icons/omega-64.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "22.0.0",
   "name": "__MSG_manifest_app_name__",
   "options_page": "options.html",
   "permissions": [ "system.cpu", "system.storage", "proxy", "tabs", "alarms", "storage", "webRequest", "webRequestBlocking", "contextMenus", "management", "notifications", "http://*/*", "https://*/*", "ftp://*/*", "\u003Call_urls>" ],
   "version": "1.2.6"
}
